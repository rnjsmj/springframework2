<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="noticeMapper">
 	<!-- 전체 목록 조회 -->
 	<select id="findAll" resultType="notice" parameterType="notice">
 		SELECT
 				NOTICE_NO,
 				NOTICE_TITLE,
 				NOTICE_CONTENT,
 				NOTICE_WRITER,
 				CREATE_DATE
 		  FROM
 		  		NOTICE
 	</select>
 	
 	
 	
 	
 	
 	<!-- 공지사항 작성 -->
 	<insert id="insert">
 		INSERT
 		  INTO
 		  		NOTICE
 		  		(
 		  		NOTICE_NO,
 		  		NOTICE_TITLE,
 		  		NOTICE_CONTENT,
 		  		NOTICE_WRITER
 		  		)
 		 VALUES
 		 		(
 		 		SEQ_NNO.NEXTVAL,
 		 		#{noticeTitle},
 		 		#{noticeContent},
 		 		#{noticeWriter}
 		 		)
 	</insert>
 </mapper>